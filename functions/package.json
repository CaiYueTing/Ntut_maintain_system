    "reporter": [
      "text"
    ]
  },
  "main": "lib/index.js",
  "dependencies": {
    "@line/bot-sdk": "^6.7.0",
    "@types/node-cache": "^4.1.3",
    "apiai": "^4.0.3",
    "axios": "^0.18.0",
    "firebase-admin": "^7.3.0",
    "firebase-functions": "^2.3.0",
    "google-auth-library": "^1.6.1",
    "googleapis": "^27.0.0",
    "inversify": "^5.0.1",
    "moment-timezone": "^0.5.25",
    "node-cache": "^4.1.1",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.6",
    "assert": "^1.4.1",
    "chai": "^4.2.0",
    "mocha": "^6.1.4",
    "nyc": "^14.0.0",
    "should": "^13.2.3",
